{:builds
 {:dev
  {:asset-path  "/js"
   :build-hooks [(helins.coload/hook {helins.fcss/coload {:src ["src/dev"]}})]
   :modules     {:main {:entries [helins.fcss.dev]}}
   :output-dir  "cljs/js"
   :target      :browser}

 :test-node
 {:build-options    {:cache-level :jars}
  :compiler-options {:reader-features #{:node}}
  :ns-regexp        ".+"
  :output-to        "compiled/node/test.js"
  :target           :node-test}}

 :deps
 {:alias [:cljs]}
 
 :dev-http
 {8000 ["fcss/dev"
        "cljs"]}
 
 :nrepl
 {:port 14563}}
